		c-0.09668-2.328125-1.200195-3.959961-3.504883-3.959961C635.772461,310.067383,634.644531,311.915039,634.572266,314.027344z"/>
	<path d="M672.613281,313.331055c0-1.87207-0.816406-3.072266-2.832031-3.072266c-2.352539,0-3.360352,1.751953-3.360352,3.792969
		v6.503906h-1.65625v-11.280273h1.631836v1.655273h0.431641c0.720703-1.295898,2.016602-1.992188,3.360352-1.992188
		c2.49707,0,4.081055,1.344727,4.081055,3.913086v7.704102h-1.65625V313.331055z"/>
	<path d="M698.556641,318.204102c0,0.863281,0.456055,1.439453,1.368164,1.439453c0.335938,0,0.671875-0.047852,0.983398-0.120117
		v1.200195c-0.503906,0.072266-1.007812,0.167969-1.511719,0.167969c-1.992188,0-2.472656-0.503906-2.472656-2.47168v-7.993164
