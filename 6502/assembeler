		l-2.47168-0.024414V318.204102z"/>
	<path d="M728.701172,312.298828c-0.09668-1.583984-1.464844-2.231445-2.856445-2.231445
		c-1.200195,0-2.592773,0.479492-2.592773,2.040039c0,0.81543,0.624023,1.223633,1.344727,1.488281
		c0.695312,0.263672,1.65625,0.431641,2.591797,0.671875c1.896484,0.431641,3.552734,1.103516,3.552734,3.143555
		c0,2.544922-2.592773,3.480469-4.920898,3.480469c-2.423828,0-4.368164-1.103516-4.583984-3.551758l1.751953-0.09668
		c0.143555,1.704102,1.488281,2.520508,3.09668,2.520508c1.319336,0,2.879883-0.52832,2.879883-2.255859
